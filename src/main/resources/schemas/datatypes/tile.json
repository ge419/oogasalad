{
  "@class": "oogasalad.model.attribute.SimpleObjectSchema",
  "name": "tile",
  "fields": [
    {
      "@class": "oogasalad.model.attribute.StringMetadata",
      "key": "type",
      "name": "View type",
      "description": "View type of tile.",
      "defaultValue": "basic",
      "editable": true,
      "viewable": true
    },
    {
      "@class": "oogasalad.model.attribute.PositionMetadata",
      "key": "position",
      "name": "Position",
      "description": "Position of tile on board.",
      "editable": true,
      "viewable": true,
      "defaultX": 0,
      "defaultY": 0
    },
    {
      "@class": "oogasalad.model.attribute.BooleanMetadata",
      "key": "owned",
      "name": "Owned",
      "description": "Whether or not tile is owned.",
      "editable": true,
      "viewable": false,
      "defaultValue": false
    },
    {
      "@class": "oogasalad.model.attribute.DoubleMetadata",
      "key": "width",
      "name": "Width",
      "description": "Width of the tile.",
      "editable": true,
      "viewable": true,
      "defaultValue": 50,
      "minValue": 0
    },
    {
      "@class": "oogasalad.model.attribute.DoubleMetadata",
      "key": "height",
      "name": "Height",
      "description": "Height of the tile.",
      "editable": true,
      "viewable": true,
      "defaultValue": 50,
      "minValue": 0
    },
    {
      "@class": "oogasalad.model.attribute.TileListMetadata",
      "key": "next",
      "name": "Next",
      "description": "Tiles to possibly go to on a forward move.",
      "editable": true,
      "viewable": true
    },
    {
      "@class": "oogasalad.model.attribute.StringMetadata",
      "key": "info",
      "name": "Info",
      "description": "Information to be stored by tile",
      "editable": true,
      "viewable": true
    },
    {
      "@class": "oogasalad.model.attribute.ColorMetadata",
      "key": "color",
      "name": "Color",
      "description": "Color of tile",
      "defaultValue": "#add8e6",
      "editable": true,
      "viewable": true
    }
  ]
}